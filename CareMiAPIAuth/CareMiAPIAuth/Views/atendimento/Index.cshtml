@model IEnumerable<CareMiAPIAuth.Models.Atendimento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.dsDescricao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.qtDias)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dsHabito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dsTempoSono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dsHereditario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dtEnvio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fgAtivo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.dsDescricao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.qtDias)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dsHabito)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dsTempoSono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dsHereditario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dtEnvio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fgAtivo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.cdAtendimento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.cdAtendimento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.cdAtendimento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
